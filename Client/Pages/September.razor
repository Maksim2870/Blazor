@page "/september"

<PageTitle>September</PageTitle>

<h2>Left until September 1st</h2>

<p role="status">Current count: @currentCount</p>

<button3 class="btn btn-primary" @onclick="IncrementCount">Clik on My days</button3>

@code {
    private int currentCount = 60;

    private void IncrementCount()
    {
        currentCount--;
    }


}



